<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">成功报名</view>
            <view class="weui-msg__desc" style="color:black;">{{ society }}--{{ department }}</view>
            <view class="weui-msg__desc">来一起组队去面试呀</view>
        </view>
        <view class="weui-msg__opr-area" id="opt">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" open-type="share" id="btn">分享</button>
                <button class="weui-btn" type="default" bindtap="back">返回</button>
            </view>
        </view>
    </view>
</view>